.indexBox {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content: center;
}

.lastpage {
  float: left;
  text-align: left;
}

.nextpage {
  float: right;
  text-align: right;
}

.nextpage::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  display: block;
  width: 1px;
  background-color: #eee;
}

.mui-content>.mui-table-view:first-child {
  margin-top: 1px;
}
