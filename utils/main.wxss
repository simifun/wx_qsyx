/* 横向滑动 */

.scroll-x-header {
  white-space: nowrap;
}

.scroll-x-view {
  display: inline-block;
  width: 22vw;
  height: 22vw;
  background: #f2f2f2;
  border-radius: 10px;
  margin-left: 32rpx;
}

button::after {
  border: none;
}
.form-btn-first .mui-table-view-cell::after,
.form-btn .mui-table-view-cell::after {
  position: absolute;
  bottom: 0;
  left: 15rpx;
  right: 15rpx;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #c8c7cc;
}

.form-btn-first {
  border: none;
