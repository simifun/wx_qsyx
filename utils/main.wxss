.imgview {
  position: relative;
  width: 20%;
  padding-bottom: 20%;
  /*margin-top: 5px*/
}

.textview {
  position: relative;
  width: 77%;
  float: left;
  padding: 5rpx 5rpx;
}

.typename {
  position: relative;
  float: right;
  color: gray;
  border: 1px solid gray;
  padding-left: 5rpx;
  padding-right: 5rpx;
  border-radius: 5rpx;
  font-size: 30rpx;
}

.imgview image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.time {
  font-size: 20rpx;
  color: gray;
  position: relative;
  float: left;
}

.author {
  padding-left: 5rpx;
  font-size: 20rpx;
  color: gray;
  position: relative;
  float: left;
}

.article-title-text {
  padding: 5rpx 15rpx;
  font-size: 33rpx;
}

.title-text {
  padding: 5rpx 5rpx;
  font-size: 40rpx;
  text-align: center;
}

.row-padding {
  padding: 10rpx 15rpx;
  font-size: 33rpx;
}

.labelview {
  padding: 10rpx 15rpx;
  font-size: 33rpx;
}

.bottom-title {
  padding: 15rpx 15rpx;
  font-size: 35rpx;
  text-align: center;
  justify-content: center;
}

.undertitle {
  position: relative;
  text-align: center;
  font-size: 35rpx;
  z-index: 3;
  padding: 10rpx 0rpx;
}

.actionview {
  position: relative;
  display: flex;
  justify-content:center;
  width: 100%;
}

.zanview {
  font-size: 30rpx;
